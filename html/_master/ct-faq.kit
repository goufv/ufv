            -webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, .05);
            box-shadow: 0 1px 1px rgba(0, 0, 0, .05);
            box-shadow: none;
            text-decoration: none;
        }
        .panel-default > .panel-heading + .panel-collapse > .panel-body {
