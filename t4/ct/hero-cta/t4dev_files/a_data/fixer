
				(function () {
					var w = window.top;
					var ua = w.navigator.userAgent.toLowerCase();

					if (w.RokoInstabot.SyncVersion == 1) return;

					try {
						var dropCache = w.localStorage.getItem('instabot-drop-cache');
						if (!dropCache) {
							w.localStorage.setItem('instabot-drop-cache', 1);

							if (localStorage.getItem('instabot-drop-cache') == 1) {
								if (ua.indexOf('trident/') > -1 ||
									ua.indexOf('msie ') > -1) {

									w.location.reload(true);
								} else if (w.fetch) {
									var scripts = [].slice.call(w.document.getElementsByTagName('script'));

									for (var i = 0; i < scripts.length; i++) {
										var script = scripts[i];

										if (script.src && (script.src.indexOf('rokoInstabot.js') != -1 || script.src.indexOf('bot.js') != -1)) {
											var xhr = new w.XMLHttpRequest;
											xhr.open('GET', script.src);
											xhr.setRequestHeader('Cache-Control', 'no-cache, no-store, max-age=0');
											xhr.setRequestHeader('Expires', 'Tue, 01 Jan 1980 1:00:00 GMT');
											xhr.setRequestHeader('Pragma', 'no-cache');
											xhr.send();

											xhr = new w.XMLHttpRequest;
											xhr.open('POST', script.src);
											xhr.setRequestHeader('Cache-Control', 'no-cache, no-store, max-age=0');
											xhr.setRequestHeader('Expires', 'Tue, 01 Jan 1980 1:00:00 GMT');
											xhr.setRequestHeader('Pragma', 'no-cache');
											xhr.send();
										}
									}
								}
							}
						}
						else {
						}
					} catch (e) {
					}
				})();
			